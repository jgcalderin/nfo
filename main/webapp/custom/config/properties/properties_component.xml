<config version="1.0">
   <!-- generic version with base functionality -->
   <scope type="do_nfop_fld">
      <component id="properties" extends="properties:webcomponent/config/library/properties/properties_component.xml">
         <pages>
         	<start>/custom/properties/properties.jsp</start>
         </pages>
         <contains>
            <component>attributes</component>
         </contains>
         <class>com.sepe.nfo.components.MyProperties</class>
      </component>
   </scope>
</config>
